{
  "name": "bk-calendar",
  "version": "2.0.0",
  "description": "Nhìn tổng quan thời khoá biểu theo ngày và các mốc thời gian khác của trường Đại học Bách Khoa - Đại học quốc gia TP.HCM",
  "main": "index.ts",
  "type": "module",
  "scripts": {
    "build": "deno compile --no-check --import-map=deno.json --allow-sys --unstable-sloppy-imports --allow-env --allow-read --allow-net --allow-write --allow-run --output \"bkcalendar.exe\" ./index.ts"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "tabletojson": "^4.1.6"
  },
  "devDependencies": {
    "typescript": "^4.5.4",
    "ts-node": "^10.9.2",
    "@types/node": "^24.3.0"
  }
}
